{
    "name": "Sputnik",
    "prettyName": "Sputnik",
    "description": "Simple and practical RSS reader",
    "version": "1.1.0",
    "author": "Jakub Szwacz <sputnik@szwacz.com>",
    "main": "index.html",
    "window": {
        "icon": "icon.png",
        "show": true,
        "toolbar": true,
        "width": 1000,
        "height": 660,
        "min_width": 800,
        "min_height": 400,
        "position": "center"
    },
    "dependencies": {
        "cheerio": "0.12.2",
        "feedparser": "0.16.x",
        "iconv-lite": "0.2.x",
        "moment": "2.8.3",
        "nedb": "0.11.1",
        "underscore": "1.7.0",
        "q": "1.0.1",
        "xmlbuilder": "0.4.x",
        "xmldoc": "0.1.x"
    },
    "config": {
        "websiteUrl": "http://sputnik.szwacz.com",
        "websiteUrlUpdate": "http://sputnik.szwacz.com/update/",
        "analyticsUrl": "http://sputnik.szwacz.com/analytics/hit.php",
        "checkUpdatesUrl": "http://sputnik.szwacz.com/check-updates/updates.json"
    }
}